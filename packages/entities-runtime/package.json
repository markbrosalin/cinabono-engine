{
    "name": "@cnbn/entities-runtime",
    "version": "1.0.2",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "private": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./store": {
            "import": "./dist/store/index.js",
            "types": "./dist/store/index.d.ts"
        },
        "./saga": {
            "import": "./dist/saga/index.js",
            "types": "./dist/saga/index.d.ts"
        },
        "./service": {
            "import": "./dist/service/index.js",
            "types": "./dist/service/index.d.ts"
        },
        "./async-cache": {
            "import": "./dist/async-cache/index.js",
            "types": "./dist/async-cache/index.d.ts"
        },
        "./dispose": {
            "import": "./dist/dispose/index.js",
            "types": "./dist/dispose/index.d.ts"
        },
        "./min-heap": {
            "import": "./dist/min-heap/index.js",
            "types": "./dist/min-heap/index.d.ts"
        },
        "./eventBus": {
            "import": "./dist/eventBus/index.js",
            "types": "./dist/eventBus/index.d.ts"
        },
        "./CallbackManager": {
            "import": "./dist/CallbackManager/index.js",
            "types": "./dist/CallbackManager/index.d.ts"
        }
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json --resolve-full-paths",
        "lint": "eslint \"**/*.ts\""
    },
    "dependencies": {
        "@cnbn/schema": "workspace:*",
        "@cnbn/utils": "workspace:*",
        "picomatch": "^4.0.3"
    },
    "devDependencies": {
        "@cnbn/typescript-config": "workspace:*",
        "@types/picomatch": "^4.0.2"
    },
    "publishConfig": {
        "access": "public"
    }
}
