{
    "name": "@cnbn/utils",
    "version": "1.0.0",
    "description": "Common utility functions for Array, Object, Tree and etc.",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "private": false,
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json --resolve-full-paths",
        "lint": "eslint \"**/*.ts\""
    },
    "dependencies": {
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@cnbn/typescript-config": "workspace:*"
    },
    "publishConfig": {
        "access": "public"
    }
}
