{
    "name": "@cnbn/engine",
    "version": "1.0.1",
    "type": "module",
    "sideEffects": false,
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs"
        }
    },
    "license": "MIT",
    "scripts": {
        "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.json --resolve-full-paths",
        "lint": "eslint \"**/*.ts\"",
        "test": "vitest"
    },
    "dependencies": {
        "@cnbn/entities-runtime": "workspace:*",
        "@cnbn/error": "workspace:*",
        "@cnbn/helpers": "workspace:*",
        "@cnbn/modules-runtime": "workspace:*",
        "@cnbn/schema": "workspace:*",
        "@cnbn/utils": "workspace:*",
        "@cnbn/config": "workspace:*",
        "@cnbn/simulation": "workspace:*",
        "okee-di-container": "workspace:*"
    },
    "devDependencies": {
        "@cnbn/typescript-config": "workspace:*",
        "@types/node": "^24.3.1",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    }
}
